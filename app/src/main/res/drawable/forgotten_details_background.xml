<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:width="1dp"
        android:color="#fff" />
    <gradient
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:angle="90"
        android:centerColor="#EAEAEA"
        android:endColor="#fEfEfE"
        android:startColor="#fAfAfA"
        android:type="linear"/>
    <padding
        android:left="0dp"
        android:right="0dp"
        android:top="5dp"
        android:bottom="0dp"/>
    <corners android:radius="20dp"/>
</shape>